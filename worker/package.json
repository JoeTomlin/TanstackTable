{
    "name": "contract-ai-worker",
    "version": "1.0.0",
    "scripts": {
      "dev": "wrangler dev",
      "deploy": "wrangler deploy",
      "db:create": "wrangler d1 create contracts_db",
      "db:init": "wrangler d1 execute contracts_db --local --file=./src/db/schema.sql",
      "db:init:remote": "wrangler d1 execute contracts_db --remote --file=./src/db/schema.sql"
    },
    "devDependencies": {
      "@cloudflare/workers-types": "^4.20241127.0",
      "wrangler": "^3.84.1",
      "typescript": "^5.3.3"
    }
  }